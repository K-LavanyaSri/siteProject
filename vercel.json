{
    "version": 2,
    "builds": [
      {
        "src": "manage.py",
        "use": "@vercel/python",
        "config": { "maxLambdaSize": "15mb" }
      },
      {
        "src": "staticfiles/**/*",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      { "src": "/static/(.*)", "dest": "staticfiles/$1" },
      { "src": "/(.*)", "dest": "manage.py" }
    ],
    "env": {
      "DJANGO_SETTINGS_MODULE": "sitepro.settings",
      "SECRET_KEY": "@@SECRET_KEY@@",
      "DEBUG": "false"
    }
  }
  